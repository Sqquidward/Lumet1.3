<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.lumet13" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:hardwareAccelerated="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Lumet13"
        android:usesCleartextTraffic="true"
        tools:targetApi="31" >
        <activity
            android:name=".MyProfileAct"
            android:exported="false"
            android:label="@string/title_activity_my_profile"
            android:theme="@style/Theme.Lumet13" >
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".Activity.Events.ProfileEventAct"
            android:exported="false"
            android:label="@string/title_activity_profile_event"
            android:theme="@style/Theme.Lumet13" >
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".Activity.Events.CreateEventAct"
            android:exported="false"
            android:label="@string/title_activity_create_event"
            android:theme="@style/Theme.Lumet13" >
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".Activity.Maps.MapsAct"
            android:exported="false"
            android:label="@string/title_activity_maps"
            android:theme="@style/Theme.Lumet13" >
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".Activity.Authorization.SignUp"
            android:exported="false"
            android:label="@string/title_activity_sign_up"
            android:theme="@style/Theme.Lumet13" />
        <activity
            android:name=".Activity.Setting.SettingAct"
            android:exported="false"
            android:label="@string/title_activity_sign_up"
            android:theme="@style/Theme.Lumet13" />
        <activity
            android:name=".Activity.Authorization.validatePswrd"
            android:exported="false"
            android:label="@string/title_activity_validate_pswrd"
            android:theme="@style/Theme.Lumet13" />
        <activity
            android:name=".Activity.Authorization.Registration"
            android:exported="true"
            android:label="@string/title_activity_registration"
            android:theme="@style/Theme.Lumet13" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Lumet13" />
    </application>

</manifest>